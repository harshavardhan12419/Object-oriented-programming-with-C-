#include <iostream>

using namespace std;

int main(int argc, char** argv) {
 

int num1,num2,gcd;  // variable declaration for input

    cout<<"Enter First Number : ";
    cin>>num1;


    cout<<"Enter Second Number : ";
    cin>>num2;

    int i = 1;

    while ( i <= num1 && i <= num2 ) {


    if ( num1 % i == 0 && num2 % i == 0 ) {

     gcd = i;

     }


       i++;

    }
    cout<<"GCD : "<<gcd ;

return 0;
}
