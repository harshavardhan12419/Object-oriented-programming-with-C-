#include <iostream> 
using namespace std; 
int main() 
{
	int a = 2, b = 3; 

	
	cout << "Before swapping the variables a= " << a 
		<< " b= " << b; 
	cout << endl; 

	
	a = a + b; 
	b = a - b; 
	a = a - b; 

 
	cout << "After swapping the variables a= " << a 
		<< " b= " << b << endl; 
	return 0; 
}
